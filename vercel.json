{
    "version": 2,
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task"
    },
    "functions": {
        "run.py": {
            "maxDuration": 60
        }
    }
}